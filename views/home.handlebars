<div id="date" style="font-size: 20px; font-weight: bold"></div>
<div class="list-group">
  {{#each events}}
    <div class="row list-group-item" data-toggle="collapse" data-target="#{{@index}}">
      <div class="col-sm-1" style="font-size: 16px; font-weight: bold">{{{startTime}}}</div>
      <div class="col-sm-11"><b>{{groupName}}</b><br>{{eventName}}</div>
    </div>
    <div id="{{@index}}" class="collapse">{{{description}}}</div>
  {{/each}}
</div>
